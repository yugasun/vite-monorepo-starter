{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["vite.config.ts", "env.d.ts", "src/*.vue", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "esModuleInterop": true,
    "ignoreDeprecations": "5.0",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node", "vite/client"]
  }
}
